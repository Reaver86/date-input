{
  "name": "date-input",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.html,.scss}\"",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.html,.scss}\"",
    "heroku-postbuild": "ng build",
    "copy-license": "cp LICENSE.txt dist/ngx-date-input",
    "copy-readme": "cp README.md dist/ngx-date-input",
    "copy-screenshot": "cp screenshot.png dist/ngx-date-input",
    "copy-files": "npm run copy-license && npm run copy-readme && npm run copy-screenshot",
    "build_lib": "ng build ngx-date-input",
    "npm_pack": "cd dist/ngx-date-input && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "prepackage": "cd projects/ngx-date-input && npm version patch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.4",
    "@angular/common": "~7.2.4",
    "@angular/compiler": "~7.2.4",
    "@angular/core": "~7.2.4",
    "@angular/forms": "~7.2.4",
    "@angular/platform-browser": "~7.2.4",
    "@angular/platform-browser-dynamic": "~7.2.4",
    "@angular/router": "~7.2.4",
    "bootstrap": "4.1.3",
    "core-js": "^2.5.4",
    "ngx-date-input": "^0.0.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.4",
    "@angular/language-service": "~7.2.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "prettier": "^1.16.4",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.4"
  }
}
